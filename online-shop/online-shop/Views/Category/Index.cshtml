@model List<Category>


<h1>Category list</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <td>
                Number
            </td>
            <td>
                Category2
            </td>
        </tr>

    </thead>

    @foreach (Category category in Model)
    {
        <tbody>
            <tr>
                <td>
                    @category.Id
                </td>
                <td>
                    @category.Name
                </td>
            </tr>
        </tbody>
    }





</table>